\r\n  position: absolute;\r\n  width: 5%;\r\n  top: 50%;\r\n  left: 0;\r\n  z-index: 1000;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin: auto;\r\n}\r\n\r\n.navbar-journey ul {\r\n  list-style: none;\r\n  padding: 0;\r\n  margin: 0;\r\n  left: -30px;\r\n}\r\n\r\n.navbar-journey ul li {\r\n  width: 15px;\r\n  position: relative;\r\n  text-align: right;\r\n  margin: 35px 0px;\r\n}\r\n\r\n.navbar-journey ul li .dot {\r\n  color: #fff;\r\n  display: block;\r\n  padding: 5px 0;\r\n}\r\n\r\n.navbar-journey ul li .dot span {\r\n  display: inline-block;\r\n  background: #ffa800;\r\n  letter-spacing: 1px;\r\n  padding: 10px 25px;\r\n  margin-right: 30px;\r\n  border-radius: 3px;\r\n  transform: translateX(30px);\r\n  opacity: 0;\r\n}\r\n\r\n.navbar-journey ul li:hover .dot span {\r\n  transform: translateX(0px);\r\n  opacity: 1;\r\n}\r\n\r\n.navbar-journey ul li .dot span::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 50%;\r\n  right: 0;\r\n  transform: translate(7px, -50%);\r\n  border-left: 7px solid #ffa800;\r\n  border-top: 7px solid transparent;\r\n  border-bottom: 7px solid transparent;\r\n  display: block;\r\n}\r\n\r\n.navbar-journey ul li .dot::before,\r\n.navbar-journey ul li .dot::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 50%;\r\n  border-radius: 50%;\r\n  display: block;\r\n  transition: 0.2s ease-out;\r\n}\r\n\r\n.navbar-journey ul li .dot::before {\r\n  height: 15px;\r\n  width: 15px;\r\n  background: rgba(255, 255, 255, 0.38);\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n}\r\n\r\n.navbar-journey ul li .dot.active-journey::before,\r\n.navbar-journey ul li:hover .dot::before {\r\n  border-color: #ffa800;\r\n  background: #ffa800;\r\n}\r\n\r\n.navbar-journey ul li .dot.active-journey::before {\r\n  height: 38px;\r\n  width: 15px;\r\n  border-radius: 50px;\r\n}\r\n\r\n.navbar-journey ul li .dot.active-journey::after,\r\n.navbar-journey ul li:hover .dot::after {\r\n  transform: translateY(-50%) scale(1);\r\n}\r\n.box-journey {\r\n  display: block;\r\n  width: 95%;\r\n}\r\n.journey-carousel .box .bigBox {\r\n  display: grid;\r\n  grid-template-columns: repeat(7, 1fr);\r\n  width: 100%;\r\n  column-gap: 20px;\r\n  background: rgba(0, 0, 0, 0.4);\r\n  border-radius: 20px;\r\n  padding: 20px;\r\n  margin-bottom: 20px;\r\n  direction: ltr;\r\n}\r\n.journey-carousel .box .boxie {\r\n  grid-column: span 4;\r\n  width: 100%;\r\n  margin: auto;\r\n}\r\n.journey-carousel .box .boxie:nth-child(2) {\r\n  grid-column: span 3;\r\n}\r\n.journey-carousel .box h2 {\r\n  font-style: normal;\r\n  font-weight: 700;\r\n  font-size: 18px;\r\n  line-height: 26px;\r\n  display: flex;\r\n  align-items: center;\r\n  color: #fff9f9;\r\n  text-shadow: 10px 10px 40px rgba(255, 255, 255, 0.25);\r\n  text-align: left;\r\n  margin-bottom: 20px;\r\n}\r\n.journey-carousel .box p {\r\n  font-style: normal;\r\n  font-weight: 400;\r\n  font-size: 14px;\r\n  line-height: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  color: #fff9f9;\r\n  text-shadow: 10px 10px 40px rgba(255, 255, 255, 0.25);\r\n  text-align: left;\r\n}\r\n.journey-carousel .box img {\r\n  width: 100%;\r\n}\r\n.flicking-pagination {\r\n  top: 10px;\r\n  left: 0;\r\n  position: absolute;\r\n  text-align: center;\r\n  width: 0%;\r\n  z-index: 2;\r\n  display: block;\r\n}\r\n.flicking-pagination-bullet.flicking-pagination-bullet-active {\r\n  background-color: #f2a65e;\r\n  height: 35px;\r\n  border-radius: 10px;\r\n  width: 12px;\r\n}\r\n.flicking-pagination-bullet {\r\n  border-radius: 50%;\r\n  cursor: pointer;\r\n  display: inline-block;\r\n  font-size: 1rem;\r\n  height: 12px;\r\n  margin: 7px 4px;\r\n  width: 12px;\r\n  background: rgba(255, 255, 255, 0.499);\r\n}\r\n.carousel-nav {\r\n  width: 120px;\r\n  margin-left: 10px;\r\n  max-height: 300px;\r\n  overflow-y: scroll;\r\n}\r\n\r\n.carousel-nav .carousel-cell-2 {\r\n  height: 80px;\r\n  width: 100px;\r\n  margin: 0 0 5px 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.carous